{
  "name": "@noahyu/lc-ui",
  "version": "1.6.9",
  "type": "module",
  "description": "LC UI",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.js"
    },
    "./styles/*": "./dist/styles/*"
  },
  "files": [
    "/dist",
    "/global.d.ts"
  ],
  "scripts": {
    "changelog": "conventional-changelog -n '../../changelog.config.js' -i CHANGELOG.md -s -r 0 -k ./package.json --commit-path ./"
  },
  "keywords": [
    "LC UI"
  ],
  "author": "Noah Yu",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Noah-Ywh/lc-library.git",
    "directory": "packages/lc-ui"
  },
  "license": "MIT",
  "engines": {
    "node": ">=22.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@vueuse/core": ">=10.7.0",
    "vue": ">=3.4.0"
  },
  "dependencies": {
    "@noahyu/lc-helpers": "workspace:*",
    "@vueuse/core": "^11.1.0",
    "konva": "^9.3.15",
    "tributejs": "^5.1.3"
  },
  "devDependencies": {
    "typescript": "^5.6.3",
    "vue": "^3.5.11"
  }
}
